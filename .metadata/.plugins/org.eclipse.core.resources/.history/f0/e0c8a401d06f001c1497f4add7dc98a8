package com.q1.form;
import javax.swing.*;

public class Calculator extends JFrame{
	public Calculator(){
		setTitle("Calculator");
		setBounds(0,0,500,500);
		setLayout(null);
		
		String dhyaney[] = {"white","seto","end"};
		JComboBox combo = new JComboBox(dhyaney);
		setBounds(100,10,150,30);
		add(combo);
		
		setVisible(true);
	}
	public static void main(String[] args){
		new Calculator();
	}

}
