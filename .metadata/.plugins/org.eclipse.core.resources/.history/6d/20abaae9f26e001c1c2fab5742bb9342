package com.q1.form;
import javax.swing.*;

public class TextArea extends JFrame{
	public TextArea(){
		setTitle("Text Area");
		setBounds(10,10,900,900);
		setLayout(null);
		
		JLabel address = new JLabel("Address: ");
		address.setBounds(15, 15, 100, 30);
		add(address);
		
		JTextArea address_field = new JTextArea(50,5);
		address_field.setBounds(15, 50, 100, 30);
		add(address_field);
	}
}
